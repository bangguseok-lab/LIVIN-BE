<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.livin.mapper.UserMapper">
    <select id="findNicknameByUserId" resultType="string" parameterType="long">
        SELECT nickname
        FROM users
        WHERE id = #{userId}
    </select>
</mapper>